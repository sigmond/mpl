/*
 * Definition of the log module. This will generate a log module data structure with a unique name.
 * The name of the log module must not be the same as any other existing log module.
 */
LOGMODULE_DEFINE(BS_MPL);

/*
 * Definition of the first (and currently only) log mask belonging to the log module defined above.
 */
LOGMASK_DEFINE(mask_0, BS_MPL,
               LOG_CAT_ERR | LOG_CAT_WRN | LOG_CAT_INF,               /* Initial category state */
               LOG_CAT_ERR | LOG_CAT_WRN | LOG_CAT_INF,               /* Channel mapping */
               0,                                                     /* Bandwidth requirements */
               0,                                                     /* Optional mask update callback */
               0,                                                     /* Optional mask update callback data */
               "MPL default logging mask",

               "ERROR",     LOG_SEC_EXTERNAL,    /* Bit description can not be changed. */
               "WARNING",   LOG_SEC_EXTERNAL,    /* Bit description can not be changed. */
               "INFO",      LOG_SEC_PARTNER,     /* Bit description can not be changed. */
               "DEBUG",     LOG_SEC_INTERNAL,    /* Bit description can not be changed. */

               "STATE",     LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "TIMER",     LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "SIGNAL",    LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "DEBUG_EXT", LOG_SEC_INTERNAL,    /* Bit description can not be changed. */

               "ASSERT_DUMP", LOG_SEC_INTERNAL,  /* Bit description can not be changed. */
               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */

               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */
               "RESERVED",  LOG_SEC_INTERNAL,    /* Bit description can not be changed. */

               "bit #15",   LOG_SEC_INTERNAL,
               "bit #14",   LOG_SEC_INTERNAL,
               "bit #13",   LOG_SEC_INTERNAL,
               "bit #12",   LOG_SEC_INTERNAL,

               "bit #11",   LOG_SEC_INTERNAL,
               "bit #10",   LOG_SEC_INTERNAL,
               "bit #9",    LOG_SEC_INTERNAL,
               "bit #8",    LOG_SEC_INTERNAL,

               "bit #7",    LOG_SEC_INTERNAL,
               "bit #6",    LOG_SEC_INTERNAL,
               "bit #5",    LOG_SEC_INTERNAL,
               "bit #4",    LOG_SEC_INTERNAL,

               "bit #3",    LOG_SEC_INTERNAL,
               "bit #2",    LOG_SEC_INTERNAL,
               "bit #1",    LOG_SEC_INTERNAL,
               "bit #0",    LOG_SEC_INTERNAL);
